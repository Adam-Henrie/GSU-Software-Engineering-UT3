<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2E516D">

    <ImageButton
        android:id="@+id/cookoutCart"
        android:layout_width="161dp"
        android:layout_height="104dp"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="52dp"
        android:background="?attr/selectableItemBackground"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wendysCart"
        app:srcCompat="@drawable/cookout" />

    <ImageButton
        android:id="@+id/mcDonaldsCart"
        android:layout_width="85dp"
        android:layout_height="96dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="24dp"
        android:background="?attr/selectableItemBackground"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/btn_CVS"
        app:layout_constraintTop_toBottomOf="@+id/btn_Taco_Bell"
        app:srcCompat="@drawable/mcdonalds" />

    <ImageButton
        android:id="@+id/wendysCart"
        android:layout_width="85dp"
        android:layout_height="96dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:background="?attr/selectableItemBackground"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toEndOf="@+id/gamestopCart"
        app:layout_constraintTop_toBottomOf="@+id/mcDonaldsCart"
        app:srcCompat="@drawable/wendys" />

    <ImageButton
        android:id="@+id/aldiCart"
        android:layout_width="104dp"
        android:layout_height="90dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="16dp"
        android:background="?attr/selectableItemBackgroundBorderless"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toEndOf="@+id/cookoutCart"
        app:layout_constraintTop_toBottomOf="@+id/mcDonaldsCart"
        app:srcCompat="@drawable/aldi_0" />

    <ImageButton
        android:id="@+id/publixCart"
        android:layout_width="81dp"
        android:layout_height="84dp"
        android:layout_marginStart="64dp"
        android:layout_marginLeft="64dp"
        android:background="?attr/selectableItemBackground"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gamestopCart"
        app:srcCompat="@drawable/publix_logo_12378_widget_logo" />

    <ImageButton
        android:id="@+id/gamestopCart"
        android:layout_width="104dp"
        android:layout_height="90dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:background="?attr/selectableItemBackgroundBorderless"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_CVS"
        app:srcCompat="@drawable/gamestop" />

    <Button
        android:id="@+id/btn_Target"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="To Target Cart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_Walmart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="To Walmart Cart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Target" />

    <Button
        android:id="@+id/btn_Taco_Bell"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="To Taco Bell Cart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Walmart" />

    <Button
        android:id="@+id/btn_CVS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="To cvs cart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Taco_Bell" />
</androidx.constraintlayout.widget.ConstraintLayout>